{
	"name": "@themion/gatsby-source-notion-api",
	"version": "0.0.0",
	"description": "Gatsby source plugin for official Notion.so API",
	"main": "index.js",
	"publishConfig": {
		"registry": "https://registry-url",
		"access": "public"
	},
	"scripts": {
		"format": "prettier --write *.{js,json,md} **/*.{js,json,md} **/**/*.{js,json,md}",
		"lint": "prettier -c *.{js,json,md} **/*.{js,json,md} **/**/*.{js,json,md}",
		"commit": "gitmoji --commit",
		"build": "echo \"No build script setup\""
	},
	"keywords": [
		"gatsby",
		"gatsby-plugin",
		"notion"
	],
	"license": "0BSD",
	"homepage": "https://github.com/Themion/gatsby-source-notion-api",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Themion/gatsby-source-notion-api.git"
	},
	"dependencies": {
		"node-fetch": "^2.6.1",
		"or-pipets": "^1.0.1",
		"yaml": "^1.10.2"
	},
	"devDependencies": {
		"gitmoji-cli": "^4.1.0",
		"husky": "^6.0.0",
		"prettier": "^2.3.0"
	},
	"husky": {
		"hooks": {
			"pre-push": "pnpm run lint"
		}
	},
	"prettier": {
		"printWidth": 100,
		"trailingComma": "all",
		"singleQuote": true,
		"useTabs": true,
		"tabWidth": 1,
		"bracketSpacing": true
	}
}
